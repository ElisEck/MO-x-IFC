@prefix :  <https://eas.iis.fraunhofer.de/AlignmentModelicaIFC#>.
@prefix ifc:  <http://standards.buildingsmart.org/IFC/DEV/IFC4/ADD1/OWL#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix brick: 	<https://brickschema.org/schema/1.1/Brick#> .
@prefix IBPSA: 	<https://eas.iis.fraunhofer.de/IBPSA#> .
@prefix LibEAS: 	<https://eas.iis.fraunhofer.de/LibEAS#> .
@prefix psd:	<https://www.linkedbuildingdata.net/IFC4-PSD> .

ifc:IfcSpace owl:equivalentClass IBPSA:Buildings.Fluid.MixingVolumes.MixingVolume .
ifc:IfcFlowSegment owl:equivalentClass IBPSA:Buildings.Fluid.FixedResistances.PressureDrop .
ifc:IfcFlowController owl:equivalentClass IBPSA:BuildingsFluidActuatorsValvesTwoWayEqualPercentage .

:RLTG a owl:Class;
	rdfs:subClassOf ifc:IfcUnitaryEquipment;
	owl:equivalentClass [
		a owl:Restriction ;
		owl:onProperty ifc:predefinedType_IfcUnitaryEquipment;
		owl:hasValue ifc:AIRHANDLER ] .
:RLTG owl:equivalentClass LibEAS:LibEAS.RLT.Anlagen.Typ1 .